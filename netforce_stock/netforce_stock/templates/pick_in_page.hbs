<v:page title="Goods Receipt">
    <v:toolbar>
        <v:button string="New Goods Receipt" icon="plus-sign" action="pick_in_edit"/>
        <v:button string="Import" action="pick_in_import" icon="download"/>
    </v:toolbar>
    <v:tabs>
        <v:tab_item string="All" action="pick_in_all"/>
        <v:tab_item string="Draft" action="pick_in_draft"/>
        <v:tab_item string="Pending" action="pick_in_pending"/>
        <v:tab_item string="Completed" action="pick_in_done"/>
    </v:tabs>
    {{{context.content}}}
</v:page>
