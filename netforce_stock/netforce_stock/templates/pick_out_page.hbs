<v:page title="Goods Issue">
    <v:toolbar>
        <v:button string="New Goods Issue" icon="plus-sign" action="pick_out_edit"/>
        <v:button string="Import" action="pick_out_import" icon="download"/>
    </v:toolbar>
    <v:tabs>
        <v:tab_item string="All" action="pick_out_all"/>
        <v:tab_item string="Draft" action="pick_out_draft"/>
        <v:tab_item string="Pending" action="pick_out_pending"/>
        <v:tab_item string="Completed" action="pick_out_done"/>
    </v:tabs>
    {{{context.content}}}
</v:page>
