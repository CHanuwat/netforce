                        </td>
                    {{/if}}
                    {{#if ../../col7}}
                        <td style="border-top: 1px solid #999;text-align:right">
                            {{currency amount7}}
                        </td>
                    {{/if}}
                    {{#if ../../col8}}
                        <td style="border-top: 1px solid #999;text-align:right">
                            {{currency amount8}}
                        </td>
                    {{/if}}
                    {{#if ../../col9}}
