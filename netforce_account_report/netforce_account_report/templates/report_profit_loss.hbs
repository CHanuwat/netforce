                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from5}}&defaults.date_to={{../../../date_to5}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount5}}
                            </a>
                        </td>
                    {{/if}}
                    {{#if ../../col6}}
                        <td style="text-align:right">
