                    {{/if}}
                    {{#if ../../col5}}
                        <td style="border-top: 1px solid #999;text-align:right">
                            {{currency amount5}}
                        </td>
                    {{/if}}
                    {{#if ../../col6}}
                        <td style="border-top: 1px solid #999;text-align:right">
