                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from8}}&defaults.date_to={{../../../date_to8}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount8}}
                            </a>
                        </td>
                    {{/if}}
                    {{#if ../../col9}}
                        <td style="text-align:right">
                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from9}}&defaults.date_to={{../../../date_to9}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount9}}
                            </a>
                        </td>
                    {{/if}}
                    {{#if ../../col10}}
                        <td style="text-align:right">
