<center>
    <h2>
        Profit &amp; Loss
    </h2>
    <h3>
        {{company_name}}
        <br/>
        {{fmt_date date_from}} to {{fmt_date date_to}}
    </h3>
</center>
<table class="table table-condensed">
    <thead class="scroll-header">
        <tr>
            <th>
            </th>
            <th style="text-align:right">
                {{fmt_date col0}}
            </th>
            {{#if col1}}
                <th style="text-align:right">
                    {{fmt_date col1}}
                </th>
            {{/if}}
            {{#if col2}}
                <th style="text-align:right">
                    {{fmt_date col2}}
                </th>
            {{/if}}
            {{#if col3}}
                <th style="text-align:right">
                    {{fmt_date col3}}
                </th>
            {{/if}}
            {{#if col4}}
                <th style="text-align:right">
                    {{fmt_date col4}}
                </th>
            {{/if}}
            {{#if col5}}
                <th style="text-align:right">
                    {{fmt_date col5}}
                </th>
            {{/if}}
            {{#if col6}}
                <th style="text-align:right">
                    {{fmt_date col6}}
                </th>
            {{/if}}
            {{#if col7}}
                <th style="text-align:right">
                    {{fmt_date col7}}
                </th>
            {{/if}}
            {{#if col8}}
                <th style="text-align:right">
                    {{fmt_date col8}}
                </th>
            {{/if}}
            {{#if col9}}
                <th style="text-align:right">
                    {{fmt_date col9}}
                </th>
            {{/if}}
            {{#if col10}}
                <th style="text-align:right">
                    {{fmt_date col10}}
                </th>
            {{/if}}
            {{#if col11}}
                <th style="text-align:right">
                    {{fmt_date col11}}
                </th>
            {{/if}}
            {{#if show_ytd}}
                <th style="text-align:right">
                    YTD
                </th>
            {{/if}}
        </tr>
    </thead>
    <tbody>
        {{#each lines context=context}}
            {{#ifeq type "group_header"}}
                <tr style="font-weight:bold">
                    <td colspan="20" {{#if padding}}style="padding-left:{{padding}}px"{{/if}}>
                        {{string}}
                    </td>
                </tr>
            {{/ifeq}}
            {{#ifeq type "account"}}
                <tr>
                    <td {{#if padding}}style="padding-left:{{padding}}px"{{/if}}>
                        {{string}}
                    </td>
                    <td style="text-align:right">
                        <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../date_from}}&defaults.date_to={{../../date_to}}&defaults.track_id={{../../track_id}}&defaults.track2_id={{../../track2_id}}">
                            {{currency amount}}
                        </a>
                    </td>
                    {{#if ../../col1}}
                        <td style="text-align:right">
                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from1}}&defaults.date_to={{../../../date_to1}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount1}}
                            </a>
                        </td>
                    {{/if}}
                    {{#if ../../col2}}
                        <td style="text-align:right">
                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from2}}&defaults.date_to={{../../../date_to2}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount2}}
                            </a>
