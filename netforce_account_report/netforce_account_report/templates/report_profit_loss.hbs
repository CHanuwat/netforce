                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from3}}&defaults.date_to={{../../../date_to3}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount3}}
                            </a>
                        </td>
                    {{/if}}
                    {{#if ../../col4}}
                        <td style="text-align:right">
                            <a href="/ui#name=report_account_trans&defaults.account_id={{id}}&defaults.date_from={{../../../date_from4}}&defaults.date_to={{../../../date_to4}}&defaults.track_id={{../../../track_id}}&defaults.track2_id={{../../../track2_id}}">
                                {{currency amount4}}
                            </a>
                        </td>
                    {{/if}}
                    {{#if ../../col5}}
