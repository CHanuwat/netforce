                                {{currency amount_ytd}}
                            </a>
                        </td>
                    {{/if}}
                </tr>
            {{/ifeq}}
            {{#ifeq type "group_footer"}}
                <tr style="font-weight:bold">
                    <td style="border-top: 1px solid #999;{{#if padding}}padding-left:{{padding}}px{{/if}}">
                        {{string}}
                    </td>
                    <td style="border-top: 1px solid #999;text-align:right">
                        {{currency amount}}
                    </td>
                    {{#if ../../col1}}
                        <td style="border-top: 1px solid #999;text-align:right">
                            {{currency amount1}}
                        </td>
                    {{/if}}
                    {{#if ../../col2}}
                        <td style="border-top: 1px solid #999;text-align:right">
                            {{currency amount2}}
                        </td>
                    {{/if}}
                    {{#if ../../col3}}
                        <td style="border-top: 1px solid #999;text-align:right">
                            {{currency amount3}}
